# Frontend Supabase configuration (use the anon key whenever possible)
VITE_SUPABASE_URL="https://your-project.supabase.co"
VITE_SUPABASE_ANON_KEY="anon-key"
# Optional: if your deploy environment only provides the service role key, the
# app will fall back to it, but avoid exposing it in production builds when
# possible.
VITE_SUPABASE_SERVICE_ROLE_KEY="service-role-key"

# Ultrapanda integration for Netlify function
ULTRAPANDA_BASE_URL="https://ht.ultrapanda.club"
ULTRAPANDA_FINGERPRINT="your-fingerprint"
ULTRAPANDA_SAVEPLAYER_SIGN="sign"
ULTRAPANDA_SAVEPLAYER_STIME="0"
ULTRAPANDA_SAVEPLAYER_TOKEN="token"
ULTRAPANDA_SAVEPLAYER_X_TOKEN="x-token"

# Optional: restrict Netlify function CORS to your deployed site
PUBLIC_SITE_ORIGIN="https://your-site.example.com"
